---
layout: layouts/base.njk
eleventyNavigation:
  key: Schedule
  order: 2
---
<div class="table-wrapper mb-8" tabindex="0" role="group" aria-labelledby="content">
  <table>
    <caption>
      <h1>{{ schedule.year }} Agenda</h1>
    </caption>
    <thead>
      <tr>
        <th>Time</th>
        <th>Description</th>
      </tr>
    </thead>
    <tbody>
    {% for item in schedule.agenda %}
      <tr>
        <td>{{ item.time }}</td>
        <td>{% if item.title %}{{ item.title }}{% endif %}</td>
      </tr>
    {% endfor %}
    <tbody>
  </table>
</div>

<h2>Sessions</h2>
<p class="text-lg"><strong>Check back soon for session info!</strong></p>

